import '2.7/Pair.adept'

func clone(this *<$T> Pair) <$T> Pair {
    result POD <$T> Pair
    result.first = this.first.clone()
    result.second = this.second.clone()
    return result
}

func clone(this *<$T, $S> AsymmetricPair) <$T, $S> AsymmetricPair {
    result POD <$T, $S> AsymmetricPair
    result.first = this.first.clone()
    result.second = this.second.clone()
    return result
}
